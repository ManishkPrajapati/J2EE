<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM 
	"hibernate-mapping-3.0.dtd">
  <hibernate-mapping>
  <class name="com.demo.model.Employee" table="emptab">
    <id name="empid">
    <generator class="assigned"></generator>
    </id>
    <property name="ename" type="string"></property>
     <property name="sal" type="double"></property>
  </class>
  <class name="com.demo.model.MyUser" table="user">
    <id name="uid">
    <generator class="assigned"></generator>
    </id>
    <property name="uname" type="string"></property>
  </class>
  </hibernate-mapping>